<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>maica__Under_18__c</fullName>
    <deprecated>false</deprecated>
    <description>Indicates if the Contact is under 18 years of age.</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(maica__Age__c), FALSE, IF(maica__Age__c &lt; 18, TRUE, FALSE))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates if the Contact is under 18 years of age.</inlineHelpText>
    <label>Under 18</label>
    <type>Checkbox</type>
</CustomField>
